<Project>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\server-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('WasmCommon_NET6')) == 'true'">
      <BaseOutputPath>bin\wasm-net6\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net6\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('ServerCommon_NET7')) == 'true'">
      <BaseOutputPath>bin\server-net7\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\server-net7\</BaseIntermediateOutputPath>
   </PropertyGroup>
   <PropertyGroup Condition="$(MSBuildProjectName.EndsWith('WasmCommon_NET7')) == 'true'">
      <BaseOutputPath>bin\wasm-net7\</BaseOutputPath>
      <BaseIntermediateOutputPath>obj\wasm-net7\</BaseIntermediateOutputPath>
   </PropertyGroup>
</Project>
